{

//规则名
"规则名": "kuyunhd",
//作者
"规则作者": "飞鱼",
//请求头UA,键名$键值，每一组用#分开，不填则默认okhttp/3.12.11，可填MOBILE_UA或PC_UA使用内置的手机版或电脑版UA
//多个请求头参数写法示例，"User-Agent$PC_UA#Referer$http://ww.baidu.com#Cookie$ser=ok",每一组用#分开。
//习惯查看手机源码写建议用手机版UA，习惯查看PC版源码写建议用电脑版UA，不支持Content-Type参数
"请求头参数":"PC_UA",
//网页编码格式默认UTF-8编码，UTF-8，GBK，GB2312
"网页编码格式":"UTF-8",
//图片是否需要代理
"图片是否需要代理":"0",  

"是否开启获取首页数据": "1",
"首页推荐链接": "https://www.kuyunhd.com",
"首页列表数组规则": ".kb-list",
"首页片单列表数组规则": ".list-sh",
"首页片单是否Jsoup写法": "1",
"首页片单标题": ".vtext&&dt&&Text",
"首页片单链接": "a&&href",
"首页片单图片": "img&&src",
"首页片单副标题": "更新+.vother&&Text",
"首页片单链接加前缀": "https://www.kuyunhd.com",
"首页片单链接加后缀": "",

 
"分类起始页码": "1",
    //{cateId}分类id
	//{area}地区
	//{by}排序
	//{class}剧情
	//{lang}语言
	//{catePg}翻页序列号
	//{year}日期
//分类链接,{cateId}是分类，{catePg}是页码,第一页没有页码的可以这样写 第二页链接[firstPage=第一页的链接]
"分类链接": "https://www.kuyunhd.com/vod/show/{cateId}/{area}/{by}/page/{catePg}/{year}.html",
//https://www.kuyunhd.com/vod/show/7/area/%E5%A4%A7%E9%99%86/by/time/page/1/year/2023.html
//分类名，分类1&分类2&分类3
"分类名称": "电影&电视剧&综艺&动漫&音乐",
//分类名替换词，替换词1&替换词2&替换词3，替换词包含英文&的用两个中文＆＆代替，示例：＆＆id=0&＆＆id=1
"分类名称替换词": "1&2&3&4&6",



//分类页面截取数据模式，0为json，其它数字为普通网页。
"分类截取模式": "1",
//分类列表数组定位，最多支持3层，能力有限，不是所有页面都能支持
//"分类列表数组规则": "body&&#publicVideoListModule",
"分类列表数组规则": ".kb-list&&li",
//分类片单信息jsoup与正则截取写法切换，只作用于html网页，1为jsoup写法(默认)，0为正则截取写法
"分类片单是否Jsoup写法":"1",
//下面这六个分类数据如果不填将调用首页那截取的配置（片单写法需一致，且取值也得一致）。
//分类片单标题
"分类片单标题": ".vtext&&dt&&Text",
//分类片单链接
"分类片单链接": "a&&href",
//分类片单图片，支持自定义图片链接
"分类片单图片": "img&&src",
//分类片单副标题
"分类片单副标题":".vother&&i&&Text",
//分类片单链接补前缀  
"分类片单链接加前缀": "https://www.kuyunhd.com",
//分类片单链接补后缀
"分类片单链接加后缀": "",
//片单链接是否直接播放，0否，1分类片单链接直接播放，2详情选集链接直接播放。
//设置成直接播放后，后面3个参数请注意该留空的请务必留空。
"链接是否直接播放": "2",
//直接播放链接补前缀
"直接播放链接加前缀": "",
//直接播放链接补后缀，设置为#isVideo=true#可强制识别为视频链接
"直接播放链接加后缀": "",
//直接播放链接设置请求头，只对直链视频有效，每一组用#分开
"直接播放直链视频请求头": "",
//下面几个参数请勿乱用。否则可能会有副作用。
//分析网页源码中有<script type="text/javascript">var player_aaaa={"flag":"play","encrypt这种源码的链接解析
//如果网页源码里没有这种请设置为0

//下面是选集规则，直接播放的不用下面
"线路列表数组规则":".nav nav-tabs mb10",
"线路列表":"li&&a",
"线路标题":"i&&Text",

"选集标题链接是否Jsoup写法":"1",
"选集列表数组规则":"#plist&&.tab-pane",
"播放列表数组规则":"",
"选集标题":"li&&a&&Text",
"选集链接":"li&&a&&href",
"选集链接加前缀":"https://www.kuyunhd.com",
"选集链接加后缀":"",
"是否反转选集序列":"",

"详情是否Jsoup写法":"1",
"类型详情":".pd-body&&dl[5]&&dd&&Text",
"年代详情":".pd-body&&dl[2]&&dd&&a&&Text",
"地区详情":".pd-body&&.clear&&dd&&a&&Text",
"演员详情":".pd-body&&dl[4]&&dd&&Text",
"简介详情":"#pcontent&&p&&Text",


